datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id         String    @id @default(uuid())
  name       String
  email      String    @unique
  password   String
  language   String
  phone      String
  pin        String?
  pinExpires DateTime?
  image      String?
  gender     String?
  birthdate  DateTime?
  created_at DateTime  @default(now())
  updated_at DateTime  @default(now())
}