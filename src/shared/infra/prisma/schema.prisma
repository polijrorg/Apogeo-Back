datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id         String   @id @default(uuid())
  name       String   @unique
  email      String
  password   String
  language   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}